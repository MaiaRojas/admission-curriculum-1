{
  "name": "admission-curriculum",
  "version": "4.0.0",
  "description": "Topics used by the admission process @Laboratoria",
  "scripts": {
    "prebuild": "npm run test",
    "build": "node scripts/build.js",
    "mdlint": "mdlint .",
    "validate": "node scripts/validate.js",
    "pretest": "npm run mdlint && npm run validate",
    "test": "npm run test-mocha && npm run test-jest",
    "test-mocha": "mocha './{,!(node_modules|build)/**/}*.spec.js' --ignore './**/node_modules/**/*.spec.js,./**/solution/**/*.spec.js,./**/sandbox/**/*.spec.js'",
    "test-jest": "jest --testPathPattern ./admission/03-prework/.*/solution/.*.spec.js --testPathIgnorePatterns ./build"
  },
  "repository": "Laboratoria/admission-curriculum",
  "keywords": [
    "javascript",
    "laboratoria",
    "admission"
  ],
  "author": "Laboratoria <sumate@laboratoria.la> (http://laboratoria.la/)",
  "license": "CC-BY-SA-4.0",
  "bugs": {
    "url": "https://github.com/Laboratoria/admission-curriculum/issues"
  },
  "dependencies": {
    "@laboratoria/curriculum-parser": "^2.5.0"
  },
  "devDependencies": {
    "@laboratoria/mdlint": "^1.1.0",
    "chai": "^4.2.0",
    "fs-extra": "^7.0.1",
    "jest": "^26.6.3",
    "minimist": "^1.2.3",
    "mocha": "^6.2.0",
    "replace-in-file": "^4.0.0"
  }
}
